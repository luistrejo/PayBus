<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.tec2.paybus.ResumenFragment"
        android:label="Inicio"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.tec2.paybus.PagarFragment"
        android:label="Pagar"
        tools:layout="@layout/fragment_pagar" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.tec2.paybus.RecargarTransferirFragment"
        android:label="Recargar/Transferir"
        tools:layout="@layout/fragment_notifications" />

    <fragment
        android:id="@+id/navigation_conf"
        android:name="com.tec2.paybus.ConfigFragment"
        android:label="Configuraciones"
        tools:layout="@layout/fragment_config" />
</navigation>